{
    "name": "tech-app",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "precache": "sw-precache --verbose --config=sw-precache-config.js",
        "build.prod": "ng build --aot --prod --progress false --base-href \"https://mushrath007.github.io/mywork-angular/\" && node_modules/.bin/ngsw-config dist ./src/ngsw-config.json && cp node_modules/@angular/service-worker/ngsw-worker.js ./dist/ngsw-worker.js",
        "ngsw-config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json",
        "ngsw-copy": "copy node_modules\\@angular\\service-worker\\ngsw-worker.js dist\\",
        "build-prod-ngsw": "ng build --prod && npm run ngsw-config && npm run ngsw-copy",
        "serve-prod-ngsw": "npm run build-prod-ngsw && http-server dist -p 8080"
    },
    "private": true,
    "dependencies": {
        "@agm/core": "^1.0.0-beta.2",
        "@agm/snazzy-info-window": "^1.0.0-beta.2",
